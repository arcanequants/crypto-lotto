{
  "installCommand": "npm install --legacy-peer-deps",
  "crons": [
    {
      "path": "/api/cron/close-hourly-draw",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/execute-hourly-draw",
      "schedule": "5 * * * *"
    },
    {
      "path": "/api/cron/close-daily-draw",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/execute-daily-draw",
      "schedule": "5 2 * * *"
    }
  ]
}
